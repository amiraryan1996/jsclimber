generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.1.x"]
}
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
model User {
  id           String    @id @default(cuid())
  sessions     Session[]
  firstName    String
  lastName     String
  email        String    @unique
  passwordHash String
}

model Session {
  id        String   @id
  expiresAt DateTime
  userId    String
  user      User     @relation(references: [id], fields: [userId], onDelete: Cascade)
}